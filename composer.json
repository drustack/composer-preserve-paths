    "autoload": {
        "psr-4": {
            "DruStack\\Composer\\PreservePaths\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DruStack\\Composer\\PreservePaths\\Tests\\": "tests/"
        }
    },
    "description": "Composer plugin for preserving custom paths and supporting nested packages",
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev",
            "dev-master": "1.0.x-dev"
        },
        "class": "DruStack\\Composer\\PreservePaths\\Plugin"
    },
    "keywords": [
        "composer-plugin",
        "installer",
        "custom path",
        "nested package"
    ],
    "license": "GPL-2.0+",
    "name": "drustack/composer-preserve-paths",
    "require": {
        "composer-plugin-api": "~1.0"
    },
    "require-dev": {
        "composer/composer": "~1.0",
        "phpunit/phpunit": "~5.7"
    },
    "type": "composer-plugin"
}
